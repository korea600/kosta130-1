<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<sqlMap namespace="reg">
<!-- ID		REQUEST		REASON		TIMES		STATUS -->

	<select id="adminReg" resultClass="admin.reg.model.RegDTO">
		select id,request,reason,times,status 
		from register
	</select>
	
	<select id="adminRegForm" parameterClass="int" resultClass="admin.reg.model.RegDTO">
		select id,name,tel,request,reason 
		from register 
		inner join member
		where id=#id#
	</select>
</sqlMap>
